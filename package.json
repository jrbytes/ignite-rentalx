  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --respawn src/shared/infra/http/server.ts",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/shared/infra/typeorm/data-source.ts",
    "typeorm:create": "npx typeorm migration:create",
    "lint": "npx eslint src --ext .ts,.tsx --fix",
    "clear:jest": "jest --clearCache",
    "test:u": "jest --testMatch=\"**/*.spec.ts\" --maxWorkers=3",
    "test:i": "NODE_ENV=test jest --runInBand --testMatch=\"**/*.test.ts\"",
    "test": "NODE_ENV=test jest --runInBand --testMatch=\"**/*.(test|spec).ts\""
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "csv-parse": "^5.2.0",
    "dayjs": "^1.11.5",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.5.0",
    "tsyringe": "^4.7.0",
    "typeorm": "^0.3.9",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^28.0.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/multer": "^1.4.7",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.17.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.0.0",
    "prettier": "^2.6.2",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.7.3"
  }
